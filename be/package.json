{
  "name": "movie-rcm-backend",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node ./node_modules/.bin/typeorm -f ./ormconfig.js",
    "typeorm:win": "ts-node -r tsconfig-paths/register node_modules\\typeorm\\cli.js",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:run": "npm run typeorm migration:run",
    "migration:run:win": "npm run typeorm:win migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "console": "ts-node -r tsconfig-paths/register src/console.ts",
    "init-data": "npm run console init-data"
  },
  "dependencies": {
    "@fast-csv/parse": "^4.3.6",
    "@hapi/joi": "^17.1.1",
    "@nestjs/axios": "^0.0.7",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^9.1.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/typeorm": "^8.0.3",
    "axios": "^1.5.1",
    "bcrypt": "^5.0.1",
    "cache-manager": "^5.2.4",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "^0.13.2",
    "cockatiel": "^3.1.1",
    "compute-cosine-similarity": "^1.1.0",
    "graphql": "^15.8.0",
    "ioredis": "^4.28.5",
    "joi": "^17.5.0",
    "lodash": "^4.17.21",
    "mathjs": "^11.11.2",
    "mysql2": "^2.3.3",
    "natural": "^0.5.4",
    "nestjs-console": "^7.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.2.45",
    "typeorm-transactional-cls-hooked": "^0.1.21",
    "uuid": "^8.3.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/hapi__joi": "^17.1.8",
    "@types/ioredis": "^4.28.5",
    "@types/jest": "27.0.2",
    "@types/mathjs": "^9.4.2",
    "@types/natural": "^5.1.3",
    "@types/node": "^17.0.21",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
